{
  "name": "catraca2",
  "version": "0.0.0",
  "main": "app.js",
  "scripts": {
    "ng": "ng",
    "rebuild": "electron-rebuild -f -w sqlite3 && electron-rebuild -f -w sharp",
    "start": "ng build --base-href ./ && electron . dev",
    "prod": "ng build --base-href ./ && rm -rf /Applications/catraca2-darwin-x64; electron-packager . --out=/Applications && open /Applications/catraca2-darwin-x64/catraca2.app",
    "release": "ng build --base-href ./ && rm -rf ./catraca2-darwin-arm64; electron-packager . && electron-installer-dmg ./catraca2-darwin-arm64/catraca2.app ControleAcesso --overwrite ",
    "build": "ng build",
    "test": "ng test",
    "update": "git reset --hard ; git pull ",
    "push": "git add . ; git commit -m lobo ; git push",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "axios": "^0.26.1",
    "body-parser": "^1.20.0",
    "bootstrap": "^5.1.3",
    "dayjs": "^1.11.1",
    "electron-settings": "^4.0.2",
    "express": "^4.17.3",
    "imagemagick": "^0.1.3",
    "pdf2pic": "^2.1.0",
    "rxjs": "~6.6.0",
    "sharp": "^0.30.3",
    "sqlite3": "^5.0.3",
    "tslib": "^2.3.0",
    "websocket": "^1.0.34",
    "ws": "^8.5.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.3",
    "@angular/cli": "~12.2.3",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "electron": "^18.0.4",
    "electron-packager": "^15.4.0",
    "electron-rebuild": "^3.2.7",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  }
}


